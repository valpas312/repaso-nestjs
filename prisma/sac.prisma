generator frasleClient {
  provider = "prisma-client-js"
  output   = "../generated/sac"
}

datasource frasle {
  provider = "postgresql"
  url      = env("DATABASE_URL_SAC")
}

model Usuario {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}