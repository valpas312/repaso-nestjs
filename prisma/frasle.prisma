generator frasleClient {
  provider = "prisma-client-js"
  output   = "../generated/frasle"
}

datasource frasle {
  provider = "postgresql"
  url      = env("DATABASE_URL_SAC")
}

model frasle {
  codigo      String  @id @frasle.VarChar(50)
  descripcion String
  precio      Decimal @frasle.Decimal(12, 2)
  categoria   String? @frasle.VarChar(100)
}

model mazfren {
  codigo      String  @id @frasle.VarChar(10)
  descripcion String
  precio      Decimal @frasle.Decimal(12, 2)
  imagen      String?
  categoria   String? @frasle.VarChar(100)
}
