generator frasleClient {
  provider = "prisma-client-js"
  output   = "../generated/mazfren"
}

datasource frasle {
  provider = "postgresql"
  url      = env("DATABASE_URL_SAC")
}

model mazfren {
  codigo      String  @id @frasle.VarChar(10)
  descripcion String
  precio      Decimal @frasle.Decimal(12, 2)
  imagen      String?
}
